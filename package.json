{
  "name": "inless",
  "version": "0.0.1",
  "description": "Make app inless code",
  "author": "Vlad Sydorenko",
  "license": "MIT",
  "main": "dist/electron.js",
  "scripts": {
    "app": "electron .",
    "frontend:build": "webpack --env.service=frontend",
    "frontend:dev": "nodemon --config .nodemon/frontend.json --exec yarn run frontend:build",
    "backend:build": "webpack --env.service=backend",
    "backend:dev": "nodemon --config .nodemon/backend.json --exec yarn run backend:build",
    "electron:build": "webpack --env.service=electron",
    "electron:dev": "nodemon --config .nodemon/electron.json --exec yarn run electron:build"
  },
  "dependencies": {
    "rxjs": "^5.5.5"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/node": "^8.5.1",
    "electron": "^1.7.9",
    "nodemon": "^1.13.3",
    "ts-loader": "^3.2.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0"
  }
}
